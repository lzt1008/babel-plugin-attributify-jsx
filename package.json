{
  "name": "babel-plugin-attributify-jsx",
  "version": "1.0.1",
  "description": "babel plugin for JSX/TSX valueless attributify",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "homepage": "https://github.com/lzt1008/babel-plugin-attributify-jsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzt1008/babel-plugin-attributify-jsx.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/lzt1008/babel-plugin-attributify-jsx/issues"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
