{
  "compilerOptions": {
    "types": ["node"],
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext"],
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
