  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@types/babel__core": "^7.1.19",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/node": "^18.7.16",
    "babel-plugin-jsx": "^1.2.0",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.10",
    "vitest": "^0.23.2"
